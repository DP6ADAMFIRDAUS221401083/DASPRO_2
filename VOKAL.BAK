program vokal;
uses crt;
var d:char;
(*type data char digunakan karena input yang dimasukkan berupa satu huruf*)
begin
  write('masukan huruf:');
  readln(d);
   case (d) of
    'u','i','e','o','a':writeln('vokal');
'b','c','d','e','f','g','h','i','j','k','l','m','n','o','p','q','r','s','t','u','v','w','x','y','z':writeln('konsonan');
   else
      writeln('bukan huruf');
  end;
readln;
end.
